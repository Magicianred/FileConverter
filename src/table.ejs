<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabela</title>
</head>
<body>

    <table align="center" border="1" cellspacing="3">
        <thead>
            <% header.forEach(col => { %>
                <th> <%= col %> </th>
            <% }) %>
        </thead>
        <trow>
            <% rows.forEach(row => { %>
                <tr>
                    <% row.forEach(element => { %>
                        <td> <%= element %> </td>
                    <% }) %>
                </tr>
            <% }) %>
        </trow>
    </table>
    
</body>
</html>